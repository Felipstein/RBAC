generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  ADMIN
  USER

  @@map("role")
}

model Account {
  id       String @id @default(uuid()) @db.Uuid
  name     String
  email    String @unique
  password String
  role     Role

  @@map("accounts")
}
